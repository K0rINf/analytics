{% extends 'base.html.twig' %}

{% block title %}Partner{% endblock %}

{% block body %}
    <h1>Partner</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partner.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ partner.name }}</td>
            </tr>
            <tr>
                <th>Stend</th>
                <td>{{ partner.stend }}</td>
            </tr>
            <tr>
                <th>IsPercent</th>
                <td>{{ partner.isPercent ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Login</th>
                <td>{{ partner.login }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ partner.password }}</td>
            </tr>
            <tr>
                <th>IsEnabled</th>
                <td>{{ partner.isEnabled ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>BankId</th>
                <td>{{ partner.bankId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('partner_index') }}">back to list</a>

    <a href="{{ path('partner_edit', {'id': partner.id}) }}">edit</a>

    {{ include('partner/_delete_form.html.twig') }}
{% endblock %}
